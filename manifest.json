{
  "manifest_version": 3,
  "name": "タブマネージャー for Zenn",
  "version": "1.0.1",
  "description": "Zennのタブをグループ化して管理できます。",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_title": "メニューを表示",
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://zenn.dev/*"
  ],
  "permissions": [
    "tabGroups"
  ]
}